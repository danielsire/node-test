{
    "name": "node-test",
    "version": "1.0.0",
    "description": "Testing node, expresse and mongo",
    "main": "index.js",
    "scripts": {
        "symlink": "ln -sf ../app ./node_modules",
        "build:symlink": "ln -sf ../dist ./node_modules && mv -Tf ./node_modules/dist ./node_modules/app",
        "start": "npm run symlink && . ./.env && nodemon app/boot.js --exec babel-node",
        "lint": "eslint .",
        "test": "echo test",
        "build": "babel app -d dist && npm run build:symlink",
        "postinstall": "npm run build"
    },
    "author": "Daniel Sire",
    "license": "ISC",
    "dependencies": {
        "babel-cli": "6.26.0",
        "babel-core": "6.26.3",
        "babel-eslint": "10.0.2",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-polyfill": "6.26.0",
        "babel-preset-es2015": "6.24.1",
        "babel-preset-stage-3": "6.24.1",
        "babel-register": "6.26.0",
        "babel-runtime": "6.26.0",
        "bluebird": "3.5.5",
        "body-parser": "1.19.0",
        "bunyan": "1.8.12",
        "bunyan-format": "0.2.1",
        "compression": "1.7.4",
        "cors": "2.8.5",
        "express": "4.17.1",
        "mongoose": "5.6.10",
        "morgan": "1.9.1"
    }
}